{
  "name": "creamy-swagger",
  "version": "1.0.0",
  "description": "マッチングアプリ「creamy」 バックエンドAPI仕様書",
  "homepage": "/creamy-swagger",
  "author": "Alesion30",
  "license": "ISC",
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn gen && react-scripts build",
    "format": "prettier -w \"**/*.{js,ts,jsx,tsx,html,json,yaml}\"",
    "gen": "swagger-merger -i ./src/docs/index.yaml -o ./public/swagger.yaml",
    "watch": "node ./src/watcher.js"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "swagger-ui-react": "^4.0.0-beta.4"
  },
  "devDependencies": {
    "@types/react-dom": "^17.0.9",
    "@types/swagger-ui-react": "^3.35.2",
    "child_process": "^1.0.2",
    "chokidar": "^3.5.2",
    "eslint": "^7.32.0",
    "prettier": "^2.3.2",
    "swagger-merger": "^1.4.3",
    "typescript": "^4.3.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
